<h1>Listing RESTURANTS</h1>

<table>
  <tr>
    <th>Companyname</th>
    <th>Address</th>
    <th>Webaddress</th>
    <th>Photo</th>
    <th></th>
    <th></th>
  </tr>

<% @products.each do |product| %>
  <tr>
    <td><%= product.companyName %></td>
    <td><%= product.Address %></td>
    <td><%= product.webAddress %></td>
    <td><%= image_tag product.photo.url(:small) %></td>
    
  </tr>
<% end %>
</table>

